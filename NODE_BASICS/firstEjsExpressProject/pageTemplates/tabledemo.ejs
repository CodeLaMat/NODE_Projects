<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles/styles.css" />
    <title>Cars</title>
  </head>
  <body>
    <h1>All Cars</h1>

    <table>
      <thead>
        <tr>
          <th>Model</th>
          <th>Licence</th>
        </tr>
      </thead>
      <tbody>
        <% cars.forEach( car=>{%>
        <tr>
          <td><%=car.model%></td>
          <td><%=car.licence%></td>
        </tr>
        <%})%>
      </tbody>
    </table>

    <a href="/">Home</a>
  </body>
</html>
